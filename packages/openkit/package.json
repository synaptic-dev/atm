  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^22.0.1",
    "@types/jszip": "^3.4.1",
    "@types/node": "^20.11.17",
    "tsup": "^8.3.5",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "ai",
    "app",
    "route",
    "openai",
    "agent",
    "tools",
    "llm"
  ],
  "author": "Synaptic",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.86.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "3.24.2"
  }
